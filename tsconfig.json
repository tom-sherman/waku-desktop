{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "allowJs": true,
    "checkJs": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "nodenext",
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "jsx": "react-jsx"
  },
  "exclude": ["dist"]
}
